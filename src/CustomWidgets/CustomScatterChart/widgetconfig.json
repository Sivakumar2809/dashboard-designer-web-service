{
  "widgetName": "CustomScatterChart",
  "guid": "6cb36272-f03e-4a75-9987-9b339e1b4096",
  "displayName": "Scatter Chart",
  "category": "Miscellaneous",
  "description": "Sample widget for getting start",
  "srcFile": "src/sourcefile.js",
  "version": "3.1.0.0",
  "dependencies": {
    "scripts": [
		"src/ej2Allcharts.min.js"
    ],
    "styles": [
		"style/widget.css"
    ]
  },
  "dataFields": [
    {
      "displayName": "Y Value(s)",
      "valueType": "measure",
      "name": "Value",
      "min": "1",
      "max": "5",
      "optional": "false"
    },
    {
      "displayName": "X Value",
      "valueType": "dimension",
      "name": "Column",
      "min": "1",
      "max": "1",
      "optional": "true"
    },
	{
      "displayName": "Tooltip",
      "valueType": "dimension",
      "name": "Tooltip",
      "min": "1",
      "max": "1",
      "optional": "true"
    }
  ],
  "functionalities": [
	{
      "header": "GridLine Settings",
      "properties": [
        {
          "displayName": "Category Axis",
          "controlType": "bool",
          "name": "showGridLineForXaxis",
          "defaultValue": "false"
        },
        {
          "displayName": "PrimaryValue Axis",
          "controlType": "bool",
          "name": "showGridLineForYaxis",
          "defaultValue": "true"
        }
      ]
    },
    {
      "header": "Basic Settings",
      "properties": [
		{
          "displayName": "Enable Animation",
          "controlType": "bool",
          "name": "enableAnimation",
          "defaultValue": "true"
        },
        {
          "displayName": "Show Legend",
          "controlType": "bool",
          "name": "showLegend",
          "defaultValue": "false"
        },
		{
          "displayName": "Legend Position",
          "controlType": "enumeration",
          "name": "legendPosition",
          "defaultValue": "Bottom",
		  "listItems": [
            "Top",
            "Bottom",
			"Left",
			"Right"
          ]
        },
        {
          "displayName": "Show Datalabels",
          "controlType": "bool",
          "name": "showDatalabels",
          "defaultValue": "false"
        },
        {
          "displayName": "DataLabel FontSize",
          "controlType": "number",
          "name": "datalabelFontSize",
          "defaultValue": "11",
          "min": "0",
          "max": "50"
        },
		{
          "displayName": "Scatter Bubble Size",
          "controlType": "number",
          "name": "bubbleSize",
          "defaultValue": "12",
          "min": "0",
          "max": "50"
        }
      ]
    },
    {
      "header": "Categoryaxis Settings",
      "properties": [
        {
          "displayName": "Show CategoryAxis",
          "controlType": "bool",
          "name": "showCategoryAxis",
          "defaultValue": "true"
        },
        {
          "displayName": "Show CategoryAxis Title",
          "controlType": "bool",
          "name": "showCategoryAxisTitle",
          "defaultValue": "false"
        },
        {
          "displayName": "Show CategoryAxis Titletext",
          "controlType": "text",
          "name": "showCategoryAxisTitletext",
          "defaultValue": " "
        },
        {
          "displayName": "Label Overflow Mode",
          "controlType": "enumeration",
          "name": "labelOverflowMode",
          "defaultValue": "Hide",
          "listItems": [
            "Hide",
            "Trim"
          ]
        },
        {
          "displayName": "Label Rotation",
          "controlType": "enumeration",
          "name": "labelRotation",
          "defaultValue": "0",
          "listItems": [
            "0",
            "-45",
            "-90",
            "45",
            "90"
          ]
        },
        {
          "displayName": "Label FontSize",
          "controlType": "number",
          "name": "xlabelFontSize",
          "defaultValue": "11",
          "min": "0",
          "max": "50"
        }
      ]
    },
    {
      "header": "Primaryaxis Settings",
      "properties": [
        {
          "displayName": "Show PrimaryAxis",
          "controlType": "bool",
          "name": "showPrimaryAxis",
          "defaultValue": "true"
        },
        {
          "displayName": "Show PrimaryAxis Title",
          "controlType": "bool",
          "name": "showPrimaryAxisTitle",
          "defaultValue": "false"
        },
        {
          "displayName": "Show PrimaryAxis Titletext",
          "controlType": "text",
          "name": "showPrimaryAxisTitletext",
          "defaultValue": " "
        },
        {
          "displayName": "Label FontSize",
          "controlType": "number",
          "name": "ylabelFontSize",
          "defaultValue": "11",
          "min": "0",
          "max": "50"
        }
      ]
    },
	{
      "header": "Scatter Color Settings",
      "properties": [
		{
          "displayName": "Scatter Series 1 Color",
          "controlType": "color",
          "name": "cc1",
          "defaultValue": "#f29e38"
        },
		{
          "displayName": "Scatter Series 2 Color",
          "controlType": "color",
          "name": "cc2",
          "defaultValue": "#ff4086"
        },
		{
          "displayName": "Scatter Series 3 Color",
          "controlType": "color",
          "name": "cc3",
          "defaultValue": "#826af9"
        },
		{
          "displayName": "Scatter Series 4 Color",
          "controlType": "color",
          "name": "cc4",
          "defaultValue": "#2d9aff"
        },
		{
          "displayName": "Scatter Series 5 Color",
          "controlType": "color",
          "name": "cc5",
          "defaultValue": "#6e53f8"
        }
	  ]
	}
  ],
  "filterSettings": {
    "masterFilter": {
      "visible": "true",
      "defaultValue": "false"
    },
    "ignoreMasterFilter": {
      "visible": "true",
      "defaultValue": "false"
    }
  }
}